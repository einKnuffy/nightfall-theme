{
    "wd": "wd",
    "name": "Nightfall",
    "colors": {
        "editor.background": "#1C1E26",
        "editor.foreground": "#B6C4F2",
        "activityBarBadge.background": "#5C6E80",
        "activityBarBadge.foreground": "#B6C4F2",
        "sideBarTitle.foreground": "#5C6E80",
        "sideBar.background": "#1C1E26",
        "sideBar.foreground": "#5C6E80",
        "activityBar.background": "#1C1E26",
        "tab.activeBackground": "#1C1E26",
        "tab.inactiveBackground": "#1C1E26",
        "badge.background": "#5C6E80",
        "breadcrumb.background": "#1C1E26",
        "breadcrumb.foreground": "#5C6E80",
        "toolbar.hoverBackground": "#1C1E26",
        "titleBar.inactiveBackground": "#1C1E26",
        "titleBar.activeBackground": "#1C1E26",
        "button.background": "#5C6E80",
        "statusBar.background": "#1c1e26",
        "statusBar.border": "#1c1e26",
        "statusBar.focusBorder": "#1c1e26",
        "button.separator": "#5C6E80",
        "sideBarSectionHeader.background": "#1c1e26",
        "dropdown.background": "#1c1e26",
        "dropdown.foreground": "#5C6E80",
        "editorGroupHeader.tabsBackground": "#1c1e26",
        "editorBracketHighlight.foreground1": "#7FDAFF",
        "editorBracketHighlight.foreground2": "#7FDAFF",
        "editorBracketHighlight.foreground3": "#7FDAFF",
        "editorBracketHighlight.foreground4": "#7FDAFF",
        "editorBracketHighlight.foreground5": "#7FDAFF",
        "editorBracketHighlight.foreground6": "#7FDAFF",
        "editorBracketMatch.border": "#7FDAFF",
        "editorLineNumber.foreground": "#505868",
        "editor.lineHighlightBorder": "#5C6E80",
        "widget.shadow": "#1c1e26",
        "editorWidget.background": "#1c1e26",
        "editorWidget.border": "#1c1e26",
        "menu.selectionBackground": "#ff0000",
        "welcomePage.background": "#1c1e26"
    },
    "semanticHighlighting": true,
    "tokenColors": [
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5C6E80"
            }
        },
        {
            "scope": ["string"],
            "settings": {
                "foreground": "#64D1A9"
            }
        },
        {
            "name": "Colors",
            "scope": ["constant.other.color"],
            "settings": {
                "foreground": "#B6C4F2"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF5370"
            }
        },
        {
            "name": "JSON Key - Level 0",
            "scope": ["source.json", "meta.structure.dictionary.json", "support.type.property-name.json"],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "JSON Key - Level 1",
            "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "JSON Key - Level 2",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#F78C6C"
            }
        },
        {
            "name": "JSON Key - Level 3",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
